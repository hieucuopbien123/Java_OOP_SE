   <children>
      <Circle fx:id="myCircle" fill="DODGERBLUE" radius="20.0" stroke="BLACK" strokeType="INSIDE" />
      <Text fx:id="myText" strokeType="OUTSIDE" strokeWidth="0.0" text="1" />
   </children>
</StackPane>
